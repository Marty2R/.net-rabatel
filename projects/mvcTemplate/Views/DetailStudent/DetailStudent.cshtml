@{
}

@model Student
<div class="container">
    <h1 class="text-center">Detail Student</h1>
    <table class="table table-bordered">
        <tr>
            <th>First Name</th>
            <td>@Model.FirstName</td>
        </tr>
        <tr>
            <th>Last Name</th>
            <td>@Model.LastName</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <th>Age</th>
            <td>@Model.Age</td>
        </tr>
        <tr>
            <th>Admission Date</th>
            <td>@Model.AdmissionDate</td>
        </tr>
    </table>
</div>